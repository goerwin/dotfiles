[
  // Starcraft 2

  {
    "keys": ["F1", "F2", "F3", "F4", "F10", "F11", "F12"],
    "if": { "appName": "SC2_x64.exe" },
    "send": ["currentKey:down", "currentKey:up"]
  },
  {
    "keys": ["1", "2", "3", "4"],
    "if": { "appName": "SC2_x64.exe", "WinL": true },
    "send": ["AltL:down currentKey AltL:up", null]
  },
  {
    "keys": ["D"],
    "if": { "appName": "SC2_x64.exe", "AltL": true },
    "send": ["CtrlL:up AltL:down C AltL:up CtrlL:down", null]
  },

  // Windows Terminal
  {
    "keys": ["1", "2", "3", "4"],
    "if": { "appName": "WindowsTerminal.exe", "WinL": true },
    "send": ["AltL:down currentKey AltL:up", null]
  },

  // Google Chrome

  {
    "keys": ["F"],
    "if": { "appName": "chrome.exe", "AltL": true, "WinL": true },
    "send": ["CtrlL:up F11 CtrlL:down", null]
  },
  {
    "keys": ["Enter"],
    "if": { "appName": "chrome.exe", "AltL": true },
    "send": ["CtrlL:up AltL:down Enter AltL:up CtrlL:down", null]
  },
  {
    "keys": ["E"],
    "if": { "appName": "chrome.exe", "AltL": true },
    "send": ["L CtrlL:up SK:Delay:10 U Enter CtrlL:down", "currentKey:up"]
  },
  {
    "keys": ["H"],
    "if": { "appName": "chrome.exe", "AltL": true, "WinL": false },
    "send": ["5", null]
  },
  {
    "keys": ["L"],
    "if": { "appName": "chrome.exe", "AltL": true, "WinL": false },
    "send": ["6", null]
  },
  {
    "keys": ["Semicolon"],
    "if": { "appName": "chrome.exe", "AltL": true },
    "send": ["L", null]
  },
  {
    "keys": ["F3"],
    "if": { "appName": "chrome.exe" },
    "send": ["CtrlL:down 5 CtrlL:up", null]
  },
  {
    "keys": ["F4"],
    "if": { "appName": "chrome.exe" },
    "send": ["CtrlL:down 6 CtrlL:up", null]
  },
  {
    "keys": ["F5"],
    "if": { "appName": "chrome.exe" },
    "send": ["AltL:down S AltL:up", null]
  },
  {
    "keys": ["F6"],
    "if": { "appName": "chrome.exe" },
    "send": ["AltL:down T AltL:up", null]
  },
  {
    "keys": ["S", "T"],
    "if": { "appName": "chrome.exe", "WinL": true },
    "send": ["AltL:down currentKey AltL:up"]
  },

  // Slack

  {
    "keys": ["P"],
    "if": { "appName": "slack.exe", "AltL": true },
    "send": ["K", null]
  }
]
