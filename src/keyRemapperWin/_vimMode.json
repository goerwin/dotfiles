[
  {
    "keys": ["Caps"],
    "if": { "isKeyDown": false, "vimMode": true, "vimModeShiftL": true },
    "setOnKeyUp": { "vimMode": false, "vimModeShiftL": false },
    "send": [null, "ShiftL:up"]
  },
  {
    "keys": ["Caps"],
    "send": [null, null],
    "set": { "vimMode": true },
    "setOnKeyUp": { "vimMode": false }
  },
  {
    "keys": ["J"],
    "if": { "vimMode": true, "Alt": true },
    "send": ["End", null]
  },
  {
    "keys": ["K"],
    "if": { "vimMode": true, "Alt": true },
    "send": ["Home", null]
  },
  {
    "keys": ["H"],
    "if": { "vimMode": true, "Win": true },
    "send": ["CtrlL:down LeftArrow CtrlL:up", null]
  },
  {
    "keys": ["L"],
    "if": { "vimMode": true, "Win": true },
    "send": ["CtrlL:down RightArrow CtrlL:up", null]
  },
  {
    "keys": ["H"],
    "if": { "vimMode": true, "Alt": true, "isAltTabMode": false },
    "send": ["CtrlL:up Home CtrlL:down", null]
  },
  {
    "keys": ["L"],
    "if": { "vimMode": true, "Alt": true, "isAltTabMode": false },
    "send": ["CtrlL:up End CtrlL:down", null]
  },
  { "keys": ["H"], "if": { "vimMode": true }, "send": ["LeftArrow", null] },
  { "keys": ["L"], "if": { "vimMode": true }, "send": ["RightArrow", null] },
  { "keys": ["J"], "if": { "vimMode": true }, "send": ["DownArrow", null] },
  { "keys": ["K"], "if": { "vimMode": true }, "send": ["UpArrow", null] },
  {
    "keys": ["F"],
    "if": { "vimMode": true },
    "set": { "vimModeShiftL": true },
    "setOnKeyUp": { "vimModeShiftL": false },
    "send": ["ShiftL:down", "ShiftL:up"]
  }
]
