{
  "remaps": "%object(_remaps.json)",
  "keyPresses": "%array(_keyPresses.json)",
  "keybindings": [
    "%dotdotdotArray(_common.json)",
    {
      "keys": ["Left"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Ctrl:down 5 Ctrl:up", null]
    },
    {
      "keys": ["Right"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Ctrl:down 6 Ctrl:up", null]
    },
    {
      "keys": ["Down"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Alt:down T Alt:up", null]
    },
    {
      "keys": ["Up"],
      "when": { "appName": "chrome.exe" },
      "fire": ["Alt:down S Alt:up", null]
    }
  ]
}
